- waitlists.each do |waitlist|
  .span-16.last.letterbox.waitlist{:class => cycle("shaded", "")}[waitlist]
    .span-5.prepend-1.name
      -#%h3{:style => "margin-bottom: 2px;"}= waitlist.customer.name
      %h4.padding-top-5
        == Waitlisted:
        = waitlist.service.andand.name
    .span-9.last.when.padding-top-5
      - waitlist.waitlist_time_ranges.each do |waitlist_time_range|
        %h5
          == #{waitlist_time_range.start_date.to_s(:appt_short_month_day_year)} - #{waitlist_time_range.end_date.to_s(:appt_short_month_day_year)},
          == #{waitlist_time_range.start_time_hours_ampm} - #{waitlist_time_range.end_time_hours_ampm}
          %br
