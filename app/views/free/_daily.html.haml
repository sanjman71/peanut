- fields_for "time_range[]", TimeRange.new do |f|
  .span-16.prepend-1.last.free_time#free_time_1
    .span-16.last.time_range#time_range_1
      = hidden_field_tag :index, 1, :name => "time_range[][index]"
      .span-4
        %span{:style => 'font-size: 13px;'}== Date:
        %br
        = f.text_field 'day', :style => 'width: 90px; font-size: 13px;', :class => 'datepicker required', :id => 'date_1'
    
      .span-3
        %span{:style => 'font-size: 13px;'}== Start Time:
        %br
        = f.text_field 'start_at', :style => 'width: 90px; font-size: 13px;', :id => 'start_at', :class => 'required'
        %br
        %span.example== e.g. 9 am
    
      .span-9.last
        %span{:style => 'font-size: 13px;'}== End Time:
        %br
        = f.text_field 'end_at', :style => 'width: 90px; font-size: 13px;', :id => 'end_at', :class => 'required'
        = link_to 'Remove', '', :class => 'remove hide', :style => 'font-size: 10px; margin-left: 10px;', :id => 'remove_1'
        %br
        %span.example== e.g. 3:30 pm
      
    .span-16.last
      %h4.success#success_1.hide== Success message here

    .span-16.last
      %h4.span-10.error#error_1.hide== Error code here
      