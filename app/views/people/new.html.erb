<h1>New resource</h1>

<% form_for(@resource) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :company_id, :value => @current_company.id %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', resources_path %>
